<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="zju.ccnt.mdsp.model.AssayItem" table="AssayItem" schema="mdsp">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="native"/>
        </id>
        <property name="assayId">
            <column name="assayId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="result">
            <column name="result" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="hint">
            <column name="hint" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="reference">
            <column name="reference" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="unit">
            <column name="unit" sql-type="varchar(40)" length="40"/>
        </property>
    </class>
</hibernate-mapping>